# We use a non-standard dir to make HA more easy:
openqrm binary: image-file-in-usr-lib
# The static binaries are part of the initrd
openqrm binary: statically-linked-binary
# license files from third-party components are listed in the copyright file
openqrm binary: extra-license-file
# intentional empty dirs
openqrm binary: package-contains-empty-directory
