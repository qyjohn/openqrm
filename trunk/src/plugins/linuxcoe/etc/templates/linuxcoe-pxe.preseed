default linux

label linux
kernel boot/@@OPENQRM_BOOTIMAGE_KERNEL@@
append ramdisk_size=131072 initrd=boot/@@OPENQRM_BOOTIMAGE_INITRD@@ languagechooser/language-name=@@LINUXCOE_INSTALL_LANGUAGE@@ auto=true preseed/url=http://@@OPENQRM_SERVER_IP_ADDRESS@@/openqrm/boot-service/lcoe/preseed.@@LINUXCOE_PROFILE_NAME@@.cfg root=/dev/ram rw debconf/priority=critical debian-installer/framebuffer=false id=@@OPENQRM_RESOURCE_ID@@ openqrm=@@OPENQRM_SERVER_IP_ADDRESS@@ selinux=0
ipappend 3

label local
LOCALBOOT 0
